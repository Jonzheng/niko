  height: 40rpx;
  opacity: 0.6;
}

.an-listen-on{
  animation: an-shad 3s linear infinite;
}

@keyframes an-shad{
  50%{box-shadow: 0 0 6px rgba(0, 0, 255, 0.2) inset;}
}

.my-progress-box{
  display: flex;
  align-items: flex-end;
  background: rgba(255, 240, 240, 0.95);
  padding-bottom: 1rpx;
  height: 0;
  width: 748rpx;
  border-bottom: 3rpx solid #337ab7;
  transition: height .6s ease-in;
